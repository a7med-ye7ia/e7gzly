      "package": "com.e7gzly1.E7gzly",
      "googleServicesFile": "./android/app/google-services (2).json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-font"
